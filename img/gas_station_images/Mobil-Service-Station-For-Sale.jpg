                                        <script type="text/javascript">
                                            function toggleIcon(e) {
                                                $(e.target)
                                                        .prev('.panel-heading')
                                                        .find(".more-less")
                                                        .toggleClass('fa-plus fa-minus');
                                            }
                                            $('.panel-group').on('hidden.bs.collapse', toggleIcon);
                                            $('.panel-group').on('shown.bs.collapse', toggleIcon);

                                            $('.sub_airtime').submit(function (e) {
                                                var artamt = parseInt($('#airtimeAmount').val());

                                                if (isNaN(artamt)) {
                                                    openModalMessage('Alert', 'Please specify an amount.');

                                                    return false;
                                                    e.preventDefault;
                                                }

                                                if ('2348135970137' == 'NO_MSISDN') {
                                                    openModalMessage('Sorry!', 'This service is currently unavailable. Please retry after some time.');
                                                    $('#form_servProd').val('');
                                                    return false;
                                                    e.preventDefault;
                                                }
                                            });

                                            var openModalMessage = function (title, content, btn_html) {
                                                $('#modalTitle').html(title);
                                                $('#modalContent').html(content);

                                                if (btn_html) {
                                                    //There is button to be added - open modal footer
                                                    $('#modalFooter').css({'display': 'block'});
                                                } else {
                                                    //Make sure modal footer is closed
                                                    $('#modalFooter').css({'display': 'none'});
                                                }

                                                $('#myModal').modal('show');
                                            }

                                            $('.buy_bundle').submit(function (e) {
                                                //Form submitted - lets prep and show modal to ask user to wait...

                                                //convert form data to js object
                                                var formData = $(this).serializeArray().reduce(function (obj, item) {
                                                    obj[item.name] = item.value;
                                                    return obj;
                                                }, {});

                                                //Check if a bundle was selected
                                                if (!formData.service_product) {
                                                    //The radio button is not checked - failed
                                                    openModalMessage('Error!', 'Please select a bundle.');
                                                    $('#form_servProd').val('');
                                                    e.preventDefault();
                                                    return false;
                                                } else {
                                                    //Radio button checked - successful
                                                    //let's return confirm
                                                    if ('2348135970137' == 'NO_MSISDN') {
                                                        openModalMessage('Sorry!', 'This service is currently unavailable. Please retry after some time.');
                                                        $('#form_servProd').val('');
                                                        e.preventDefault();
                                                        return false;
                                                    } else {
                                                        var properties = formData.service_product;
                                                        var propArray = properties.split('[]');
                                                        var service_product = propArray[0] + '[]' + propArray[1];
                                                        var plan = propArray[2];
                                                        var price = propArray[3];

                                                        if (plan && price) {
                                                            var confm = 'To activate <b>' + plan + '</b>, <b>' + price + '</b> will be deducted from your account. Are you sure you want to continue?';
                                                        } else {
                                                            var confm = 'You have selected the option to pay per MB used. Are you sure you want to continue?';
                                                        }

                                                        $('#form_servProd').val(service_product);
                                                        openModalMessage('Confirm', confm, 1);
                                                    }
                                                }

                                                return false;
                                                e.preventDefault();
                                            });

                                            //Confirmed buy
                                            $('.confirmed_buy_bundle').submit(function (e) {
                                                openModalMessage('Please wait...', 'Your request is being processed.');
                                            });


                                            $('.close_modal').click(function () {
                                                $('#myModal').modal('hide');
                                            });

                                        </script>
                                                                            </body>
                                    </html>



